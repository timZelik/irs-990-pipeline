# Streamlit Cloud Secrets Configuration
# Copy this file to secrets.toml and add your actual values

# Dashboard password
[general]
password = "YOUR_PASSWORD_HERE"

# Supabase connection (HTTP API)
# Get URL from: Supabase Dashboard > Settings > API > Project URL
# Get Key from: Supabase Dashboard > Settings > API > anon public key
[database]
url = "https://YOUR_PROJECT.supabase.co"
key = "YOUR_ANON_KEY"
