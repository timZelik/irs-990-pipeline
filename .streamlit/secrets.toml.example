# Streamlit Cloud Secrets Configuration
# Copy this file to secrets.toml and add your actual values

# Dashboard password
[general]
password = "YOUR_PASSWORD_HERE"

# Supabase PostgreSQL connection
# Get this from Supabase Dashboard: Settings > Database > Connection string
# Use the "URI" format and replace <password> with your database password
[database]
url = "postgresql://postgres:YOUR_PASSWORD@db.YOUR_PROJECT.supabase.co:5432/postgres"
